<div class="nav">
    <h3 class="logo">{{@blog.title}}</h3>
    <ul class="menu">
        {{#foreach navigation}}
            <li class="nav-{{slug}}{{#if current}} nav-current{{/if}}"><a href="{{url absolute="true"}}">{{label}}</a></li>
        {{/foreach}}
    {{#if @labs.subscribers}}
        <li><a class="subscribe-button" href="{{@blog.url}}/subscribe/">Subscribe</a><li>
    {{else}}
        <li><a class="subscribe-button icon-feed" href="{{@blog.url}}/rss/">Subscribe</a></li>
    {{/if}}
    </ul>
</div>
<span class="nav-cover"></span>
